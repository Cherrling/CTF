   "source": [
    "# 定义输出文件名\n",
    "output_file = 'ports.txt'\n",
    "\n",
    "# 打开文件以写入\n",
    "with open(output_file, 'w') as file:\n",
    "    # 遍历常用端口范围\n",
    "    for port in range(1, 10000):\n",
    "        file.write(f\"{port}\\n\")  # 每个端口号后面加换行符\n",
    "\n",
    "print(f\"端口号已输出到 {output_file}\")\n"
   ]
  }
